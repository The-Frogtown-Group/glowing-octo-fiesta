{
  "blocked_domains": [],

  "context_rules": [
    {
      "name": "YouTube in Google Slides",
      "target_domains": [
        "youtube.com",
        "youtube-nocookie.com",
        "googlevideo.com",
        "ytimg.com"
      ],
      "when_referer_contains": [
        "slides.google.com"
      ],
      "message": "Videos are not allowed in slides"
    },
    {
      "name": "YouTube in Google Docs",
      "target_domains": [
        "youtube.com",
        "youtube-nocookie.com",
        "googlevideo.com"
      ],
      "when_referer_contains": [
        "docs.google.com"
      ],
      "message": "Videos are not allowed in docs"
    }
  ],

  "blocked_url_patterns": [],

  "max_download_bytes": 10485760,

  "blocked_content_types": [],

  "blocked_extensions": [],

  "blocked_js_keywords": [
    "cryptonight",
    "coinhive",
    "crypto-loot",
    "eaglercraft",
    "minecraft"
  ]
}
